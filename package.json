  },
  "homepage": "https://github.com/erikbeauvalot-sg/nutrivault#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.5.1",
    "dotenv": "^17.2.3",
    "multer": "^2.0.2",
    "react-chartjs-2": "^5.3.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0"
  },
