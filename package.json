{
  "name": "nutrivault",
  "version": "8.5.16",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "npm run test:all",
    "test:backend": "cd backend && npm test",
    "test:backend:watch": "cd backend && npm run test:watch",
    "test:backend:integration": "cd backend && npm run test:integration",
    "test:backend:coverage": "cd backend && npm run test:coverage",
    "test:frontend": "cd frontend && npm test",
    "test:frontend:watch": "cd frontend && npm run test:watch",
    "test:frontend:coverage": "cd frontend && npm run test:coverage",
    "test:all": "npm run test:backend && npm run test:frontend",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikbeauvalot-sg/nutrivault.git"
