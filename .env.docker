# NutriVault Docker Environment Configuration
# Copy this to .env before running docker-compose

# JWT Secrets (MUST be at least 32 characters!)
# Generate with: openssl rand -base64 48
JWT_SECRET=change-this-secret-must-be-32-chars-minimum-docker-example
REFRESH_TOKEN_SECRET=change-refresh-secret-32-chars-minimum-docker-example

# CORS Configuration
# Add your domain(s) here, comma-separated
ALLOWED_ORIGINS=http://localhost:3001,http://localhost:80,https://yourdomain.com
