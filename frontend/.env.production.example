# ===================================
# NutriVault Frontend - Production
# ===================================

# API Configuration
VITE_API_URL=https://api.yourdomain.com

# Application Info
VITE_APP_NAME=NutriVault
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

# Analytics (if applicable)
VITE_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
VITE_HOTJAR_ID=XXXXXXX

# Error Tracking
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project
VITE_SENTRY_ENVIRONMENT=production

# ===================================
# Notes
# ===================================
# - All VITE_ prefixed variables are embedded in the build
# - Never include secrets in frontend environment variables
# - These values will be public in the JavaScript bundle
