{
  "name": "nutrivault-backend",
  "version": "8.6.13",
  "description": "NutriVault Backend API - POC",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "NODE_ENV=test jest --watch --runInBand",
    "test:integration": "NODE_ENV=test jest integration --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage --runInBand",
    "test:verbose": "NODE_ENV=test jest --verbose --runInBand",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "admin:create": "node src/cli/manage-admin.js create",
    "admin:reset-password": "node src/cli/manage-admin.js reset-password",
    "seed": "node src/seed.js"
  },
  "keywords": [
    "nutrition",
    "healthcare",
    "api"
  ],
  "author": "NutriVault Team",
  "license": "GPL-2.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@mistralai/mistralai": "^1.13.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "firebase-admin": "^13.6.1",
    "googleapis": "^170.1.0",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "pdfkit": "^0.17.2",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "ua-parser-js": "^2.0.8",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5",
    "supertest": "^7.2.2"
  }
}
